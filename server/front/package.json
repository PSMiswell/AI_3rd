{
  "name": "vue-abchat",
  "version": "0.2.0",
  "private": false,
  "main": "dist/abchat.common.js",
  "module": "dist/abchat.esm.js",
  "unpkg":"dist/abchat.min.js",
  "browser": {
    "./sfc": "src/abchat.vue"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name abchat ./src/index.js"

  },
  "dependencies": {
    "axios": "^0.19.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-service": "^3.11.0",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ]
}